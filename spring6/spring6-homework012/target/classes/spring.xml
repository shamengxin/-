<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userDao" class="com.shamengxin.spring6.dao.UserDao"/>
    <bean id="userService" class="com.shamengxin.spring6.service.UserService" autowire="byName"/>

    <bean id="accountDao" class="com.shamengxin.spring6.dao.AccountDao"/>
    <bean id="accountService" class="com.shamengxin.spring6.service.AccountService" autowire="byType"/>

</beans>